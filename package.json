{
  "name": "@uirouter/angular-hybrid",
  "version": "6.0.0",
  "scripts": {
    "example": "cd example && npm i && npm start",
    "clean": "shx rm -rf lib _bundles",
    "compile": "npm run clean && ngc",
    "build": "run-s clean compile bundle fixmaps:*",
    "bundle": "rollup -c && rollup -c --environment MINIFY",
    "fixmaps:lib": "tweak_sourcemap_paths -a --include 'lib/**/*.js.map' 'lib-esm/**/*.js.map'",
    "fixmaps:bundle": "tweak_sourcemap_paths -p ../src --include '_bundles/**/*.js.map'",
    "release": "release --deps @uirouter/core @uirouter/angular @uirouter/angularjs",
    "test": "npm run build",
    "test:downstream": "npm run build && test_downstream_projects",
    "prepublishOnly": "npm run build",
    "precommit": "pretty-quick --staged"
  },
  "license": "MIT",
  "dependencies": {
    "@uirouter/angular": "2.0.0",
    "@uirouter/angularjs": "1.0.17",
    "@uirouter/core": "5.0.19"
  },
  "peerDependencies": {
    "@angular/core": "^5.0.0 || ^6.0.0",
    "@angular/upgrade": "^5.0.0 || ^6.0.0",
    "angular": "^1.5.0"
  },
  "devDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^6.0.1",
    "@angular/core": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/upgrade": "^5.0.0",
    "@types/angular": "^1.6.48",
    "@types/jquery": "^3.3.1",
    "@uirouter/publish-scripts": "^2.3.20",
    "angular": "^1.7.2",
    "husky": "^0.14.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "shx": "^0.2.2",
    "typescript": "^2.9.2",
    "zone.js": "^0.8.20"
  },
  "main": "_bundles/ui-router-angular-hybrid.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/angular-hybrid.git"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/common",
      "@angular/compiler",
      "@angular/compiler-cli",
      "@angular/core",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/upgrade"
    ]
  }
}
