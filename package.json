{
  "name": "@uirouter/angular-hybrid",
  "version": "18.0.0",
  "scripts": {
    "example": "cd example && npm i && npm start",
    "clean": "shx rm -rf lib _bundles dist",
    "build": "npm run clean && ng-packagr -p ng-package.json",
    "release": "release --deps @uirouter/core @uirouter/angular @uirouter/angularjs",
    "test": "npm run build",
    "test:downstream": "npm run build && test_downstream_projects",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "checkPeerDependencies": {
    "ignore": [
      "postcss"
    ]
  },
  "peerDependencies": {
    "@angular/core": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "@angular/upgrade": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "@uirouter/angular": "^14.0.0",
    "@uirouter/angularjs": "^1.0.30",
    "@uirouter/core": "^6.1.0",
    "angular": "^1.5.0"
  },
  "resolutions": {
    "@uirouter/core": "^6.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^18.0.6",
    "@angular/common": "^18.0.6",
    "@angular/compiler": "^18.0.6",
    "@angular/compiler-cli": "^18.0.6",
    "@angular/core": "^18.0.6",
    "@angular/platform-browser": "^18.0.6",
    "@angular/platform-browser-dynamic": "^18.0.6",
    "@angular/upgrade": "^18.0.6",
    "@types/angular": "^1.8.9",
    "@types/jquery": "^3.5.6",
    "@uirouter/angular": "^14.0.0",
    "@uirouter/angularjs": "^1.1.0",
    "@uirouter/core": "^6.1.0",
    "@uirouter/publish-scripts": "^2.6.3",
    "@uirouter/rx": "^1.0.0",
    "angular": "^1.8.3",
    "husky": "^4.3.8",
    "ng-packagr": "^18.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^2.0.1",
    "rxjs": "^7.4.0",
    "rxjs-compat": "^6.6.7",
    "shx": "^0.3.3",
    "typescript": "5.4.5",
    "tslib": "2.6.2",
    "zone.js": "^0.14.4"
  },
  "distDir": "dist",
  "sideEffects": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/angular-hybrid.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
