{
  "name": "@uirouter/angular-hybrid",
  "version": "15.0.2",
  "scripts": {
    "example": "cd example && npm i && npm start",
    "clean": "shx rm -rf lib _bundles dist",
    "build": "npm run clean && ng-packagr -p ng-package.json",
    "release": "release --deps @uirouter/core @uirouter/angular @uirouter/angularjs",
    "test": "npm run build",
    "test:downstream": "npm run build && test_downstream_projects",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "checkPeerDependencies": {
    "ignore": [
      "postcss"
    ]
  },
  "peerDependencies": {
    "@angular/core": "^14.0.0 || ^15.0.0",
    "@angular/upgrade": "^14.0.0 || ^15.0.0",
    "@uirouter/angular": "^11.0.0",
    "@uirouter/angularjs": "^1.0.30",
    "@uirouter/core": "^6.1.0",
    "angular": "^1.5.0"
  },
  "resolutions": {
    "@uirouter/core": "^6.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^15.2.3",
    "@angular/common": "^15.2.3",
    "@angular/compiler": "^15.2.3",
    "@angular/compiler-cli": "^15.2.3",
    "@angular/core": "^15.2.3",
    "@angular/platform-browser": "^15.2.3",
    "@angular/platform-browser-dynamic": "^15.2.3",
    "@angular/upgrade": "^15.2.3",
    "@types/angular": "^1.8.2",
    "@types/jquery": "^3.5.6",
    "@uirouter/angular": "11.0.0",
    "@uirouter/angularjs": "^1.0.30",
    "@uirouter/core": "^6.1.0",
    "@uirouter/publish-scripts": "^2.6.0",
    "@uirouter/rx": "^1.0.0",
    "angular": "^1.8.3",
    "husky": "^4.3.8",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^2.0.1",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "shx": "^0.3.3",
    "typescript": "4.8.4",
    "zone.js": "0.11.4"
  },
  "distDir": "dist",
  "sideEffects": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/angular-hybrid.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
